  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "{0: 1, 2: 1, 4: 2, 6: 1, 9: 1}\n"
     ]
    }
   ],
   "source": [
    "def convertVector(l):\n",
    "    d = {}\n",
    "    for e in l:\n",
    "        if e != 0:\n",
    "            pos = l.index(e)\n",
    "            d.update({pos:e})\n",
    "            l[pos] = None\n",
    "    return d\n",
    "\n",
    "l = [1, 0, 1 , 0, 2, 0, 1, 0, 0, 1, 0]\n",
    "d = convertVector(l)\n",
    "print(d)"
   ]
  },
