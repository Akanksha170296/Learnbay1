   "source": [
    "def convertDictionary(d):\n",
    "    l=[]\n",
    "    if d=={}:\n",
    "        l=[]\n",
    "    else:\n",
    "        k=list(d.keys())\n",
    "        v=list(d.values())\n",
    "        for i in range(k[-1]+1):\n",
    "            if i in k:\n",
    "                l.append(d[i])\n",
    "            else:\n",
    "                l.append(0)        \n",
    "    print(l)\n",
    "\n",
    "d={0: 1, 2: 1, 4: 2, 6: 1, 9: 1}\n",
    "convertDictionary(d)"
   ]
  },
