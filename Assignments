   "source": [
    "def invertDictionary(d):\n",
    "    key = list(d.keys())    # [a,b,c,d]\n",
    "    val = list(d.values())  # [1,2,3,2]\n",
    "    dout = {}\n",
    "    for i in val:\n",
    "        vali = val.index(i)\n",
    "        if i in dout:\n",
    "            dout[i].append(key[vali])\n",
    "        else:\n",
    "            dout[i] = [key[vali]]\n",
    "        val[vali] = None\n",
    "    return dout\n",
    "    \n",
    "dout = invertDictionary({'a':2, 'b':1, 'c':2, 'd':1})\n",
    "print(dout)"
   ]
  },
